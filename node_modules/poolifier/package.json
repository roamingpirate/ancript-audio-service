{
  "$schema": "https://json.schemastore.org/package",
  "name": "poolifier",
  "version": "4.4.5",
  "description": "Fast and small Node.js Worker_Threads and Cluster Worker Pool",
  "license": "MIT",
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs"
    }
  },
  "publishConfig": {
    "directory": "package"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "volta": {
    "node": "22.11.0",
    "pnpm": "9.13.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/poolifier/poolifier.git"
  },
  "keywords": [
    "nodejs",
    "worker pool",
    "thread pool",
    "worker_threads",
    "cluster",
    "concurrency",
    "performance",
    "cpu",
    "computing",
    "scheduling",
    "parallelism",
    "eventloop"
  ],
